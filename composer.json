{
  "name": "andre1502/net_utilities",
  "version": "0.3.3",
  "description": "Library to combine common function",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "andre1502"
    }
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3",
    "guzzlehttp/guzzle": "^7.8.1",
    "illuminate/support": "^9|^10|^11",
    "lcobucci/clock": "^3.2",
    "lcobucci/jwt": "^4.3|^5.3",
    "symfony/http-foundation": "^6.3|^6.4|^7.0|^7.1"
  },
  "require-dev": {
    "nunomaduro/collision": "^8.1.1",
    "pestphp/pest": "^2.34.7",
    "pestphp/pest-plugin-laravel": "^2.4"
  },
  "scripts": {
    "analyse": "echo \"Static analysis not configured yet.\" && exit 0",
    "test": "vendor/bin/pest --order-by random",
    "test-coverage": "echo \"Test coverage not configured yet.\" && exit 0"
  },
  "autoload": {
    "psr-4": {
      "Andre1502\\NetUtilities\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Andre1502\\NetUtilities\\NetUtilitiesProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
